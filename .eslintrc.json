{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["react-app", "prettier"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
        "react", "prettier"],
    "rules": {
        "semi": "error",
        "prettier/prettier": [
            "error", 
            {
        "printWidth": 80,
        "trailingComma": "es5",
        "semi": false,
        "jsxSingleQuote": true,
        "singleQuote": true,
        "useTabs": true
            }
        ]
    }
}
